{
  "name": "Adonis-Starter",
  "version": "0.0.0",
  "description": "Adonis starter",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "init": "node ace migration:run && node ace seed && npm run docs",
    "refresh": "node ace migration:refresh && node ace seed",
    "docs": "rimraf public/docs && apidoc -i app/ -o public/docs",
    "test": "(export ENV_PATH=.env.test|| set ENV_PATH=.env.test) && node ace migration:run && node ace seed && node ace test --bail"
  },
  "author": "hlozancic",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^4.0.7",
    "@adonisjs/ally": "^2.0.2",
    "@adonisjs/antl": "^2.0.4",
    "@adonisjs/auth": "^2.0.10",
    "@adonisjs/bodyparser": "^2.0.1",
    "@adonisjs/cors": "^1.0.3",
    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/framework": "^4.0.28",
    "@adonisjs/ignitor": "^1.0.14",
    "@adonisjs/lucid": "^4.1.3",
    "@adonisjs/mail": "^3.0.5",
    "@adonisjs/validator": "^5.0.0",
    "@adonisjs/vow": "^1.0.14",
    "adonis-cache": "^0.3.1",
    "adonis-throttle-requests": "^1.0.1",
    "mysql": "^2.15.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "rimraf": "^2.6.2",
    "sqlite3": "^3.1.13"
  },
  "apidoc": {
    "description": "",
    "header": {
      "filename": "apidoc.md"
    },
    "order": [
      "Auth",
      "User"
    ]
  },
  "autoload": {
    "App": "./app"
  }
}
